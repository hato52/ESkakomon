<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hato.eskakomon">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@drawable/es_icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/es_icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".CheckedQuestionActivity"></activity>
        <activity android:name=".MissedQuestionActivity" />
        <activity android:name=".QuestionViewActivity" />
        <activity android:name=".ResultActivity" />
        <activity android:name=".QuestionActivity" />
        <activity
            android:name=".MainActivity"
            android:label="ES過去問"
            android:theme="@style/NoActionBarTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>